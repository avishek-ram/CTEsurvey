        function get_radio_value(val){
            switch(val) {
                case val == "0":
                    return "Strongly Disagree";
                    break;
                case val == "1":
                return "Disagree";
                    break;
                case val == "2":
                return "Agree";
                    break;
                case val == "3":
                return "Strongly Agree";
                    break;
                default:
                    return "unknown"
            }
        }

        $("#btn_submit").on("click", function(){

            function get_radio_value(val){
                switch(val) {
                    case "0":
                        return "Strongly Disagree";
                        break;
                    case  "1":
                        return "Disagree";
                        break;
                    case  "2":
                        return "Agree";
                        break;
                    case "3":
                        return "Strongly Agree";
