                        swal({
                            title: "Form Incomplete!",
                            text: "Please Ensure Form is Completed",
                            icon: "warning",
                        });
                        return;
                    }

                    if(has_clinicals == "No"){
                        clinical_q1 = null;
                    }
                }
                else{
                    has_clinicals = null;
                    clinical_q1 = null;
                }
                
                //continue execution once validation completed

                //build data here
                var survey_data = {
                    survey_type: "{{survey_type}}",
                    course_id : "{{course_id}}",
                    age : age,
                    gender : gender,
                    is_first_year : is_first_year,
                    campus : campus,
                    college : college,
                    course : course,
                    programme : programme,
                    course_q1 : get_radio_value(course_q1),
                    course_q2: get_radio_value(course_q2),
                    course_q3 : get_radio_value(course_q3),
                    course_q4 : get_radio_value(course_q4),
                    course_q5 : get_radio_value(course_q5),
                    teacher_q1: get_radio_value(teacher_q1),
                    teacher_q2 :get_radio_value(teacher_q2),
                    teacher_q3 : get_radio_value(teacher_q3),
                    technical_q1 : get_radio_value(technical_q1),
                    audio : audio,
                    videos: videos,
                    readings: readings,
                    recorded_lectures: recorded_lectures,
                    accessibility_input: accessibility_input,
                    has_labs : has_labs,
