                    allowClear: true, 
                    placeholder:"Select a College",
                    allowClear: true,
                    width:"55%"
                });

                $("#college_select").val("")
                $("#college_select").trigger("change");
                
                $('#Age_select').select2(
                {
                    dropdownParent: $('#Age_select').parent(),
                    allowClear: true, 
                    placeholder:"Select Your Answer",
                    allowClear: true,
                    width:"200px"
                });

                $("#Age_select").val("")
                $("#Age_select").trigger("change");


                $('#course_select').select2(
                {
                    dropdownParent: $('#course_select').parent(),
                    placeholder: "Select a Course",
                    allowClear: true,
                    width:"200px"
                });

                $("#course_select").val("")
                $("#course_select").trigger("change");

                $('#campus_select').select2(
                {
                    dropdownParent: $('#campus_select').parent(),
                    placeholder: "Select Campus",
                    allowClear: true,
                    width:"200px"
                });

                $("#campus_select").val("")
                $("#campus_select").trigger("change");
                
            });
        })();

        $('#college_select').on('change', function(){
            var college = $('#college_select').val();
            $.get( "/SurveyForm/api/get_course_college_mapping.php", function( data ) {
                //console.log(data);
                if(data){
                    data = JSON.parse(data);
                    $('#course_select').select2('destroy');
                    $('#course_select').html("");
                    _.each(data, function(x){
                        if(college == '' || college == null ){
                            $('#course_select').append('<option' +' value="'+ x.course_code+'" >'+x.course_code +'</option>');
                        }
                        else if(x.college == college){
                            $('#course_select').append('<option' +' value="'+ x.course_code+'" >'+x.course_code +'</option>');
                        }
                    });
                    $('#course_select').select2(
                    {
                        dropdownParent: $('#course_select').parent(),
                        allowClear: true,
                        placeholder: "Select a Course",
                        width:"200px"
                    });
                    $("#course_select").val("")
                    $("#course_select").trigger("change");
                }
            });
        });

        $('[name="has_labs"]').on('change', function(){
            var opt_selected = $('[name="has_labs"]:checked').val(); 
            if(opt_selected =='Yes'){
                $("#lab_row").css("display","block");
            }else if(opt_selected == 'No'){
                $("#lab_row").css("display","none");
            }
        })
  </script>
